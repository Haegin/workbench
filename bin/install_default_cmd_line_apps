#!/bin/sh

brew install xctool        # xctool - http://nshipster.com/xctool/
brew install go            # golang - https://golang.org

brew install youtube-dl    # youtube-dl - https://github.com/rg3/youtube-dl
npm install --global emoj  # emoj - https://github.com/sindresorhus/emoj
brew install asciinema     # asciicinema - https://asciinema.org
brew tap victor/whereami && brew install whereami # https://github.com/victor/whereami

go get -u github.com/schachmat/wego # wego # requires a valid ~/.wegorc




