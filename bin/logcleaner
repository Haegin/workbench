#!/bin/bash
if [ -d "/u/apps/housetrip" ]; then
  rm -rf /u/apps/housetrip/log/test* 
  rm -rf /u/apps/housetrip/log/cucumber* 
  rm -rf /u/apps/housetrip/log/development*

  touch /u/apps/housetrip/log/development.log
  touch /u/apps/housetrip/log/test.log
fi

if [ -d "/u/apps/housetrip-parallel-test" ]; then
  rm -rf /u/apps/housetrip-parallel-test/log/test* 
  rm -rf /u/apps/housetrip-parallel-test/log/cucumber* 
  rm -rf /u/apps/housetrip-parallel-test/log/development*

  touch /u/apps/housetrip-parallel-test/log/development.log
  touch /u/apps/housetrip-parallel-test/log/test.log
fi
